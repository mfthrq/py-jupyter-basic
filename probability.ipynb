   "source": [
    "from fractions import Fraction\n",
    "\n",
    "def P(event, space):\n",
    "    favorable = set.intersection\n",
    "    cases = len\n",
    "    return Fraction(cases(favorable(event, space)),\n",
    "                    cases(space))"
   ]
  },
